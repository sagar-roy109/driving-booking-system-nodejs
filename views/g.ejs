<!DOCTYPE html>
<html lang="en">
<!-- head  -->
<%- include('layouts/head'); -%>

	<body>
		<!-- include navbar  -->
		<%- include('layouts/nav'); -%>
			<!-- Page Header-->
			<header class="masthead" style="background-image: url('/assets/img/about-bg.jpg')">
				<div class="container position-relative px-4 px-lg-5">
					<div class="row gx-4 gx-lg-5 justify-content-center">
						<div class="col-md-10 col-lg-8 col-xl-7">
							<div class="page-heading">
								<h1>G Test Booking</h1>
							</div>
						</div>
					</div>
				</div>
			</header>
			<!-- Main Content-->
			<main class="mb-4">
				<div class="container">
					<div class="row">
						<form action="/data/show" method="GET">
							<div class="form-group">
								<label for="lcnumber">Enter Your License Number </label>
								<input required type="text" class="form-control" id="lcnumber" placeholder="Enter Name"
									name="lc">
							</div>
							<button type="submit" class="btn btn-primary mt-2">Search</button>
						</form>
						<% if(typeof blogposts !='undefined' ){
							if(blogposts==null ){ %>
							<span class="mt-2 mb-2" style="color:red"> User Not Found ! </span>

							<% } else{ %>
								<%- include('layouts/data'); -%>
									<% } } %>

					</div>
				</div>
			</main>
			<!-- Footer-->
			<%- include ('layouts/footer'); -%>
				<!-- scripts  -->
				<%- include('layouts/scripts'); -%>
	</body>

</html>
